{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "composite": true,
    "outDir": "./lib",
    "lib": ["ESNext"],
    "skipLibCheck": true,
    "strict": true,
    "newLine": "lf",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "emitDeclarationOnly":true
  }
}