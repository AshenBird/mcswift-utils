      "import": "./lib/esm/child_process.mjs",
      "require": "./lib/cjs/child_process.cjs",
      "types": "./types/child_process.d.ts"
    },
    "./path": {
      "default": "./lib/esm/path.mjs",
      "import": "./lib/esm/path.mjs",
      "require": "./lib/cjs/path.cjs",
      "types": "./types/path.d.ts"
    }
  },
  "keywords": [],
